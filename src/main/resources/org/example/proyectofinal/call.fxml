<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.ImageView?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.proyectofinal.VideoCall.VideoCallController"
            prefHeight="400.0" prefWidth="600.0">
    <HBox spacing="10">
        <VBox alignment="CENTER">
            <Label text="Client Call" />
            <ImageView fx:id="consumerImageView" fitHeight="300.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
        </VBox>
        <VBox alignment="CENTER">
            <Label text="Sender Call" />
            <ImageView fx:id="senderImageView" fitHeight="300.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
        </VBox>
    </HBox>
</AnchorPane>
